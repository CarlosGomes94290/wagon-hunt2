<%= simple_form_for @product do |f| %>
    <%= f.input :name, placeholder: "kudoz", label:"Which product" %>
    <%= f.input :url, placeholder: "http://getkudoz.com", label:"Wich Url" %>
    <%= f.input :tagline, placeholder: "Tinder for job search", label:"Wich tagline" %>
    <%= f.button :submit, "Ajouter", class: "btn btn-primary" %>
    <% end %>
